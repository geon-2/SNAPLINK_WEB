{
  "routes": [
    {
      "src": "/.well-known/(.*)",
      "dest": "/.well-known/$1"
    },
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "link.snaplink.run"
        }
      ],
      "dest": "/api/link"
    },
    {
      "src": "/",
      "has": [
        {
          "type": "host",
          "value": "support.snaplink.run"
        }
      ],
      "dest": "/customer-service"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}